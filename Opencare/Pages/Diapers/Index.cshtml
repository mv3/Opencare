@page
@model Opencare.Pages.Diapers.IndexModel

@{
    ViewData["Title"] = "Index";
}

<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Diaper[0].Time)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Diaper[0].Wet)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Diaper[0].Dirty)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Diaper[0].Note)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model.Diaper) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Time)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Wet)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Dirty)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Note)
            </td>
            <td>
                <a asp-page="./Edit" asp-route-id="@item.id">Edit</a> |
                <a asp-page="./Details" asp-route-id="@item.id">Details</a> |
                <a asp-page="./Delete" asp-route-id="@item.id">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
